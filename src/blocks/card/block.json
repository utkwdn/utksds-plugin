{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utkwds/card",
	"version": "0.1.0",
	"title": "Card",
	"category": "design",
	"icon": "list-view",
	"description": "A flexible and extensible content container for highlighting pieces of content.",
	"supports": {
		"html": false
	},
	"attributes": {
		"blockName": {
			"type": "string",
			"default": "utkwds/card"
		},
		"color": {
			"type": "string",
			"default": "#F6F6F6"
		},
		"colorSlug": {
			"type": "string",
			"default": "bg-light"
		},
		"cardOutline": {
			"type": "boolean",
			"default": false
		},
		"textColor": {
			"type": "string",
			"default": "text-dark"
		}
	},
	"providesContext": {
    "card/blockName": "blockName",
		"card/colorSlug": "colorSlug",
		"card/cardOutline": "cardOutline"
	},
	"textdomain": "utkwds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}