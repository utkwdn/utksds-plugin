{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utkwds/strip",
	"version": "0.1.0",
	"title": "Content Strip",
	"category": "utdesign_layout",
	"icon": "editor-table",
	"description": "",
	"supports": {
		"html": false
	},
	"attributes": {
		"color": {
			"type": "string",
			"default": "#58595B"
		},
		"colorSlug": {
			"type": "string",
			"default": "strip-smokey"
		},
		"textColor": {
			"type": "string",
			"default": "text-white"
		},
		"spacing": {
			"type": "integer",
			"default": 0
		},
		"padding":{
			"type": "string",
			"default": "py-0"
		},
		"imageUrl": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "utkwds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}